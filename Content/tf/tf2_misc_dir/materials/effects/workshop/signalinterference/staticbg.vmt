"SpriteCard"
{
	"$baseTexture" "effects/workshop/signalinterference/staticbg"
	"$vertexalpha" "1"
	"$vertexcolor" "1"
	"$translucent" "1"
	"$forcerefract" 1
 	"$bluramount" "0"
	"$nofog" "1"
	"$vertexcolormodulate" "1"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 20
		}
	}
}