"LightmappedGeneric" 

{ 
   "$basetexture" "tools/toolsblack"
    "%keywords" "tf"
	
	"%tooltexture" "dev/tfwater_normal"
	"$translucent" "1"

	"$envmap" "env_cubemap"	
	"$envmaptint" "[ .4 .4 .4 ]"
	"$bumpmap" "moon\mercury\mercury1_normal"

	"$surfaceprop" "water"
	"$color" "[ 0 0 0 ]"
	"$bumpframe" "0"
	
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
	}
	
}