"Water"
{
	"%keywords" "tf"
	
	"%tooltexture" "dev/tfwater_normal"
	"%compilewater" 1
	"%compileKeepLight" 1
	"$abovewater" 1
	"$translucent" 1

	"$envmap" "env_cubemap"

	"$normalmap" "water/scrolling_water01_normal"

	"$surfaceprop" "water"
	"$bottommaterial" "water/water_well_beneath.vmt"
	"$bumpframe" "0"

	"$fogenable" 1
//	"$fogcolor" "{52 137 135}"
//	"$fogcolor" "{148 167 195}" //Light sky blue
	"$fogcolor" "{104 141 193}" //saturated blue
	"$fogstart" "1"
	"$fogend" "400"

	 "$temp" "[0 0]"
	 "$curr" 0.0
	 "$curr2" 0.0
	
	"$normalmapalphaenvmapmask" "1"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$normalmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 10.00
		}

		"Sine"
		{
			"sineperiod" "24"
			"sinemin" -0.5
			"sinemax" 0.5
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "16"
			"sinemin" 0.5
			"sinemax" -0.5
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$bumptransform"
		}  

		"TextureScroll"
		{
			"texturescrollvar" "$bumptransform"
			"texturescrollrate" 0.1
			"texturescrollangle" 315.00
		}
	}
}
