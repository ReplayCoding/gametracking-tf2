"LightmappedGeneric"
{

	<dx90
	{
		 	"$fallbackmaterial" "water/water_hydro_cheap_dx80"
	}

	"%tooltexture" "dev/tfwater_normal"
	"%keywords" "tf"
	"%compilewater" "1"
	"%compileKeepLight" 1



	"$basetexture" "custom/patagonia/water_blue_base_clear"
	
	"$basetexturetransform" "center .5 .5 scale 2 2 rotate 0 translate 0 0"
	
	"$bumpmap" "water/tfwater001_normal"
	"$normalmap" "water/tfwater001_normal"
	//"$translucent" "1"
	"$surfaceprop" "water"
	"$envmap" "env_cubemap"
	"$envmaptint"    "[0.3 0.3 0.3]"
//	"$envmapcontrast" ".5"
	"$color" "[0.1 0.1 0.13]"

	"$abovewater" "1"

	"$fogenable" 1
	"$fogcolor" "{51 43 13}"
	"$fogstart" "-100"
	"$fogend" "400"

	"$bottommaterial" "water/water_well_beneath.vmt"
	"$normalmapalphaenvmapmask" "1"
	"$bumpframe" "0"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 24
		}
	}
