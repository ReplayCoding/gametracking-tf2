"vertexlitgeneric"
{
	"$basetexture" "models/props_arctic/television/static"
    	"$selfillum" "1"

	"$envmap"	"env_cubemap"
	"$envmaptint" "[ .100 .100 .100]"
	"$phong" "1"
	"$phongexponent" "500"
	"$phongboost" "2"	
	"$phongfresnelranges"	"[.25 .5 1]"
	"$basemapalphaphongmask" "1"

	"$rimlight" "0"
	"$rimlightexponent" "1"	
	"$rimlightboost" "0.2"

	"Proxies"
		{
		"AnimatedTexture"
			{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 24.00
			}
		}


}
