"UnlitGeneric"
{
	"$basetexture" "effects\spookyridge\portal_test2"
//	"$detail" "effects\animatedsheen\animatedsheen0"
	"$detail" "effects\spookyridge\portal_test_caustic"
	"$detailscale" "1"
	"$detailblendmode" "8"
//	"$detailblendfactor" "0"
	"$detailtexturetransform" "center .5 .5 scale 1.5 1.5 rotate 225 translate 0 0"
	$vertexcolor 1
	$vertexalpha 1
	$translucent 1
	$additive	1

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30.00
		}
	}
}