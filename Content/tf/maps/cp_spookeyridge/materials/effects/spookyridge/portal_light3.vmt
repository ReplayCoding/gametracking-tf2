"UnlitGeneric"
{
	"$basetexture" "effects\spookyridge\portal_light3"
	"$detail" "effects\spookyridge\portal_test_caustic"
	"$detailscale" "1"
	"$detailblendmode" "8"
	"$detailtexturetransform" "center .5 .5 scale 1.5 1.5 rotate 45 translate 0 0"

	$vertexcolor 1
	$vertexalpha 1
	$translucent 1
	$additive	1

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30.00
		}
	}
}