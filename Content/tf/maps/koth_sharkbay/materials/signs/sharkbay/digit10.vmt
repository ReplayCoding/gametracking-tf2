"LightmappedGeneric"
{
	"$basetexture" "signs/sharkbay/digit10"
	"$decal" 1
	//"$decalsecondpass" 1 
	"$decalscale" 0.25
	"$translucent" 1
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$alphatest" 1
	//"$ignorez" 1

 	Proxies
  	{
    		ToggleTexture
    		{
      			toggleTextureVar "$basetexture"
      			toggleTextureFrameNumVar "$frame"
			toggleShouldWrap "1"
    		}
  	}
	"%keywords" "tf, zeus"
}
