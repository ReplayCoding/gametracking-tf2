"LightMappedGeneric"
{
%CompileWater 1
	"$basetexture"	"custom/darkmarsh/darkmarsh_water_surface"
	"$bumpmap" "moon\mercury\mercury1_normal"
	"$envmap" "env_cubemap"
	"$surfaceprop" "water"
	"$translucent" "1"
   	"$envmaptint"    "[ .1 .1 .1 ]"
   	"$envmapsaturation" "[ 0 0 0 ]"
	"%keywords" "tf"
	"$bottommaterial" "water/water_underworld_beneath.vmt"
	"$color" "[ .1 .1 .1 ]"
 	"Proxies"
	{
        	"AnimatedTexture"
        	{
        	 "animatedtexturevar" "$bumpmap"
           	 "animatedtextureframenumvar" "$bumpframe"
           	 "animatedtextureframerate" 30.00
        	}
    	}
}