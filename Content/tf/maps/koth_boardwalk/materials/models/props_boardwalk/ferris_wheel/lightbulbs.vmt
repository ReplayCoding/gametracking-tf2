{
	"$basetexture" "models/props_boardwalk/ferris_wheel/lightbulbs"

//	"$basemapalphaphongmask" "1"

  
	"$phong" "1"
	"$phongexponent" "50"
	"$phongboost" "4"	
//	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
