{
	"$basetexture"	"models/props_cove/chain_001"
	"$surfaceprop"	"metal"
	"$basemapalphaphongmask" 1
	
	//phong properties
	"$phong"	"1"
	"$phongboost"	".5"
	"$phongexponent" "20"
	"$phongfresnelranges"	"[.5 1 2]"
	
//	"$rimlight"	"1"
