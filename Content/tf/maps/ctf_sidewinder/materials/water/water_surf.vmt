"LightmappedGeneric"
{

	"%tooltexture" "dev/tfwater_normal"
	"%keywords" "tf"
	"%compilewater" "1"
	"%compileKeepLight" 1


	"$basetexture" "water/water_hydro_base"
	"$bumpmap" "water/tfwater001_normal"
	"$normalmap" "water/tfwater001_normal"
	"$translucent" "1"
	"$surfaceprop" "water"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"
//	"$envmapcontrast" ".5"

	"$abovewater" "0"

//	"$fogenable" 1
//	"$fogcolor" "{51 43 13}"
//	"$fogstart" "-100"
//	"$fogend" "400"

	$underwateroverlay "effects/water_warp_2fort"
	
	
	"$normalmapalphaenvmapmask" "1"
	"$bumpframe" "0"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}
	}
}

